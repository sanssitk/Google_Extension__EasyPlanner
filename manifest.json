{
    "name": "Easy Planner",
    "version": "1.0",
    "description": "Plan your day and motivate you to take action!",
    "permissions": ["storage", "contextMenus", "tabs"],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/Decom_Logo.png",
        "32": "images/Decom_Logo.png",
        "48": "images/Decom_Logo.png",
        "128": "images/Decom_Logo.png"
      }
    },
    "icons": {
      "16": "images/Decom_Logo.png",
      "32": "images/Decom_Logo.png",
      "48": "images/Decom_Logo.png",
      "128": "images/Decom_Logo.png"
    },
    "manifest_version": 2
  }
  